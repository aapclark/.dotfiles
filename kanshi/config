{
  output eDP-1 disable
  output DP-5 mode 3440x1440@60Hz position 0,0
  output DP-2 mode 1920x1080@60Hz position 1600,1440
  exec swaymsg workspace 1, move workspace to DP-2
  exec swaymsg workspace 2, move workspace to DP-2
  exec swaymsg workspace 3, move workspace to DP-5
  exec swaymsg workspace 4, move workspace to DP-5
}

{
  output eDP-1 disable
  output DP-4 mode 3440x1440@60Hz position 0,0
}

 {
  output eDP-1 disable
  output DP-4 mode 3440x1440@60Hz position 0,0
  output DP-2 mode 1920x1080@60Hz position 220,1440
  exec swaymsg workspace 1, move workspace to DP-2
  exec swaymsg workspace 2, move workspace to DP-2
  exec swaymsg workspace 3, move workspace to DP-4
  exec swaymsg workspace 4, move workspace to DP-4
}

{
  output eDP-1 enable mode 1920x1080@60Hz
}
